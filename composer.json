{
  "name": "mch/choicelist-type-bundle",
  "type": "symfony-bundle",
  "description": "Form type extension for independant ChoiceType dataset and fields configuration inside a CollectionType",
  "license": "MIT",
  "keywords": ["Symfony", "form", "field", "choicetype", "collection"],
  "authors": [
    {
      "name": "Maxime Chagnolleau",
      "email": "maxime.chagnolleau@free.fr"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/http-kernel": "^3.4|^4.0",
    "symfony/form": "^3.4|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": { "Mch\\ChoiceListTypeBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Mch\\ChoiceListTypeBundle\\Tests\\": "tests/" }
  }
}
